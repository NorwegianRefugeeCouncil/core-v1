                        <label class="form-check-label" for="HasCognitiveDisabilityAll">
                            All
                        </label>
                    </div>
                    <div class="form-check form-check-inline">
                        <input class="form-check-input" type="radio" value="true"
                               id="HasCognitiveDisabilityYes"
                               name="has_cognitive_disability"
                               {{if .Options.IsHasCognitiveDisabilitySelected }}checked="checked"{{end}}>
                        <label class="form-check-label" for="HasCognitiveDisabilityYes">
