        <h6 class="mt-2">Protection</h6>
        <div class="row">
            <!-- Protection Concerns -->
            <div class="form-group col-6">
                <label class="form-text">Presents Protection Concerns</label>
                <div>
                    <div class="form-check form-check-inline">
                        <input class="form-check-input" type="radio" value=""
                               id="PresentsProtectionConcernsAll"
                               name="presents_protection_concerns"
                               {{if eq .Options.PresentsProtectionConcerns nil }}checked="checked"{{end}}>
                        <label class="form-check-label" for="PresentsProtectionConcernsAll">
                            All
                        </label>
                    </div>
                    <div class="form-check form-check-inline">
                        <input class="form-check-input" type="radio" value="true"
                               id="PresentsProtectionConcernsYes"
                               name="presents_protection_concerns"
                               {{if .Options.IsPresentsProtectionConcernsSelected }}checked="checked"{{end}}>
                        <label class="form-check-label" for="PresentsProtectionConcernsYes">
                            Yes
                        </label>
                    </div>
                    <div class="form-check form-check-inline mb-3">
                        <input class="form-check-input" type="radio" value="false"
                               id="PresentsProtectionConcernsNo"
                               name="presents_protection_concerns"
                               {{if .Options.IsNotPresentsProtectionConcernsSelected }}checked="checked"{{end}}>
                        <label class="form-check-label" for="PresentsProtectionConcernsNo">
                            No
                        </label>
                    </div>
                </div>
            </div>
            <!-- End Protection Concerns -->
            <!-- Displacement Status -->
            <div class="col-6">
                <label class="form-label" for="DisplacementStatus">Displacement Status</label>
                <select id="DisplacementStatus" name="displacement_status" class="form-control">
                    <option value="" {{if .Options.DisplacementStatuses.IsEmpty }}selected{{end}}>

                    </option>
                    <option value="idp"
                            {{if .Options.DisplacementStatuses.Contains "idp" }}selected{{end}}>
                        Internally Displaced Person
                    </option>
                    <option value="refugee"
                            {{if .Options.DisplacementStatuses.Contains "refugee" }}selected{{end}}>
                        Refugee
                    </option>
                    <option value="host_community"
                            {{if .Options.DisplacementStatuses.Contains "host_community" }}selected{{end}}>
                        Host Community
                    </option>
                    <option value="returnee"
                            {{if .Options.DisplacementStatuses.Contains "returnee" }}selected{{end}}>
                        Returnee
                    </option>
                    <option value="asylum_seeker"
                            {{if .Options.DisplacementStatuses.Contains "asylum_seeker" }}selected{{end}}>
                        Asylum Seeker
                    </option>
                    <option value="non_displaced"
                            {{if .Options.DisplacementStatuses.Contains "non_displaced" }}selected{{end}}>
                        Non Displaced
                    </option>
                    <option value="other"
                            {{if .Options.DisplacementStatuses.Contains "other" }}selected{{end}}>
                        Other
                    </option>
                </select>
            </div>
            <!-- End Displacement Status -->
        </div>

        <h6 class="mt-2">Contact</h6>
        <div class="row">
            <!-- Address -->
            <div class="form-group mb-3 col-6">
                <label class="form-label" for="Address">Address</label>
                <input id="Address"
                       name="address"
                       type="text"
                       class="form-control"
                       placeholder="Search by address"
                       value="{{.Options.Address}}">
            </div>
            <!-- End Address -->

            <!-- Email -->
            <div class="form-group mb-3 col-3">
                <label class="form-label" for="Email">Email</label>
                <input id="Email"
                       name="email"
                       type="text"
                       class="form-control"
                       placeholder="Search by email"
                       value="{{.Options.Email}}">
            </div>
            <!-- End Email -->

            <!-- Phone -->
            <div class="form-group mb-3 col-3">
                <label class="form-label" for="PhoneNumber">Phone Number</label>
                <input id="PhoneNumber"
                       name="phone_number"
                       type="text"
                       class="form-control"
                       placeholder="Search by phone number"
                       value="{{.Options.PhoneNumber}}">
            </div>
            <!-- End Phone -->
        </div>

        <h6 class="mt-2">Data Collection</h6>
        <div class="row">
            <!-- EngagementContext -->
            <div class="form-group mb-3 col-3">
                <label class="form-label" for="EngagementContext">Engagement Context</label>
                <select id="EngagementContext" name="engagement_context" class="form-control">
                    <option value="" {{if .Options.EngagementContext.IsEmpty }}selected{{end}}>
                    </option>
                    <option value="houseVisit"
                            {{if .Options.EngagementContext.Contains "houseVisit" }}selected{{end}}>
                        House Visit
                    </option>
                    <option value="fieldActivity"
                            {{if .Options.EngagementContext.Contains "fieldActivity" }}selected{{end}}>
                        Field Activity
                    </option>
                    <option value="inOffice"
                            {{if .Options.EngagementContext.Contains "inOffice" }}selected{{end}}>
                        In-Office
                    </option>
                    <option value="remoteChannels"
                            {{if .Options.EngagementContext.Contains "remoteChannels" }}selected{{end}}>
                        Remote Channels
                    </option>
                    <option value="referred"
                            {{if .Options.EngagementContext.Contains "referred" }}selected{{end}}>
                        Referred
                    </option>
                    <option value="other"
                            {{if .Options.EngagementContext.Contains "other" }}selected{{end}}>
                        Other
                    </option>
                </select>
            </div>
            <!-- End EngagementContext -->

            <!-- CollectionAdministrativeArea1 -->
            <div class="form-group mb-3 col-3">
                <label class="form-label" for="CollectionAdministrativeArea1">Collection Administrative Area 1</label>
                <input id="CollectionAdministrativeArea1"
                       name="collection_administrative_area_1"
                       type="text"
                       placeholder="Search by Collection Administrative Area 1"
                       class="form-control"
                       value="{{.Options.CollectionAdministrativeArea1}}">
            </div>
            <!-- End CollectionAdministrativeArea1 -->

            <!-- CollectionAdministrativeArea2 -->
            <div class="form-group mb-3 col-3">
                <label class="form-label" for="CollectionAdministrativeArea2">Collection Administrative Area 2</label>
                <input id="CollectionAdministrativeArea2"
                       name="collection_administrative_area_2"
                       type="text"
                       placeholder="Search by Collection Administrative Area 2"
                       class="form-control"
                       value="{{.Options.CollectionAdministrativeArea2}}">
            </div>
            <!-- End CollectionAdministrativeArea2 -->

            <!-- CollectionAdministrativeArea3 -->
            <div class="form-group mb-3 col-3">
                <label class="form-label" for="CollectionAdministrativeArea3">Collection Administrative Area 3</label>
                <input id="CollectionAdministrativeArea3"
                       name="collection_administrative_area_3"
                       type="text"
                       placeholder="Search by Collection Administrative Area 3"
                       class="form-control"
                       value="{{.Options.CollectionAdministrativeArea3}}">
            </div>
            <!-- End CollectionAdministrativeArea3 -->

            <!-- CollectionOffice -->
            <div class="form-group mb-3 col-3">
                <label class="form-label" for="CollectionOffice">Collection Office</label>
                <input id="CollectionOffice"
                       name="collection_office"
                       type="text"
                       placeholder="Search by Collection Office"
                       class="form-control"
                       value="{{.Options.CollectionOffice}}">
            </div>
            <!-- End CollectionOffice -->
        </div>

        <div class="row">
            <!-- Free Field 1-->
            <div class="col col-3">
                <div class="form-group mb-3">
                    <label class="form-label" for="FreeField1">Free Field 1</label>
                    <input id="FreeField1"
                           name="free_field_1"
                           type="text"
                           placeholder="Search by Free Field 1"
                           class="form-control"
                           value="{{.Options.FreeField1}}">
                </div>
            </div>
            <!-- End Free Field 1-->
            <!-- Free Field 2-->
            <div class="col col-3">
                <div class="form-group mb-3">
                    <label class="form-label" for="FreeField2">Free Field 2</label>
                    <input id="FreeField2"
                           name="free_field_2"
                           type="text"
                           placeholder="Search by Free Field 2"
                           class="form-control"
                           value="{{.Options.FreeField2}}">
                </div>
            </div>
            <!-- End Free Field 1-->
            <!-- Free Field 3-->
            <div class="col col-3">
                <div class="form-group mb-3">
                    <label class="form-label" for="FreeField3">Free Field 3</label>
                    <input id="FreeField3"
                           name="free_field_3"
                           type="text"
                           placeholder="Search by Free Field 3"
                           class="form-control"
                           value="{{.Options.FreeField3}}">
                </div>
            </div>
            <!-- End Free Field 3-->
            <!-- Free Field 4-->
            <div class="col col-3">
                <div class="form-group mb-3">
                    <label class="form-label" for="FreeField4">Free Field 4</label>
                    <input id="FreeField4"
                           name="free_field_4"
                           type="text"
                           placeholder="Search by Free Field 4"
                           class="form-control"
                           value="{{.Options.FreeField4}}">
                </div>
            </div>
            <!-- End Free Field 4-->
            <!-- Free Field 5-->
            <div class="col col-3">
                <div class="form-group mb-3">
                    <label class="form-label" for="FreeField5">Free Field 5</label>
                    <input id="FreeField5"
                           name="free_field_5"
                           type="text"
                           placeholder="Search by Free Field 5"
                           class="form-control"
                           value="{{.Options.FreeField5}}">
                </div>
            </div>
            <!-- End Free Field 5-->
        </div>

        <h6 class="mt-2">Services</h6>
        <div class="row">
            <!-- Service CC -->
            <div class="form-group mb-3 col-3">
                <label class="form-label" for="ServiceCC">Service CC </label>
                <select id="ServiceCC" name="service_cc" class="form-control">
                    <option value="" {{if .Options.ServiceCC.IsEmpty }}selected{{end}}>
                    </option>
                    <option value="shelter_and_settlements"
                            {{if .Options.ServiceCC.Contains "shelter_and_settlements" }}selected{{end}}>
                        Shelter & Settlements
                    </option>
                    <option value="wash"
                            {{if .Options.ServiceCC.Contains "wash" }}selected{{end}}>
                        WASH
                    </option>
                    <option value="protection"
                            {{if .Options.ServiceCC.Contains "protection" }}selected{{end}}>
                        Protection
                    </option>
                    <option value="education"
                            {{if .Options.ServiceCC.Contains "education" }}selected{{end}}>
                        Education
                    </option>
                    <option value="icla"
                            {{if .Options.ServiceCC.Contains "icla" }}selected{{end}}>
                        ICLA
                    </option>
                    <option value="lfs"
                            {{if .Options.ServiceCC.Contains "lfs" }}selected{{end}}>
                        LFS
                    </option>
                    <option value="cva"
                            {{if .Options.ServiceCC.Contains "cva" }}selected{{end}}>
                        CVA
                    </option>
                    <option value="other"
                            {{if .Options.ServiceCC.Contains "other" }}selected{{end}}>
                        Other
                    </option>
                </select>
            </div>
            <!-- End Service CC -->
        </div>
        <div class="row">
            <!-- Service Requested Date  -->
            <div class="col-6">
                <label class="form-label">Service Requested Date </label>
                <div class="row">
                    <div class="col-3 input-group mb-2 w-50">
                        <div class="input-group-prepend">
                            <div class="input-group-text">From</div>
                        </div>
                        <input type="date" name="service_requested_date_from" class="form-control"
                               id="ServiceRequestedDateFrom"
                               value="{{if .Options.ServiceRequestedDateFrom}}{{.Options.ServiceRequestedDateFrom.Format "2006-01-02"}}{{end}}">
                    </div>

                    <div class="col-3 input-group mb-2 w-50">
                        <div class="input-group-prepend">
                            <div class="input-group-text">To</div>
                        </div>
                        <input type="date" name="service_requested_date_to" class="form-control"
                               id="ServiceRequestedDateTo"
                               value="{{if .Options.ServiceRequestedDateTo}}{{.Options.ServiceRequestedDateTo.Format "2006-01-02"}}{{end}}">
                    </div>
                </div>
            </div>
            <!-- End Service Requested Date  -->

            <!-- Service Delivered Date  -->
            <div class="col-6">
                <label class="form-label">Service Delivered Date </label>
                <div class="row">
                    <div class="col-3 input-group mb-2 w-50">
                        <div class="input-group-prepend">
                            <div class="input-group-text">From</div>
                        </div>
                        <input type="date" name="service_delivered_date_from" class="form-control"
                               id="ServiceDeliveredDateFrom"
                               value="{{if .Options.ServiceDeliveredDateFrom}}{{.Options.ServiceDeliveredDateFrom.Format "2006-01-02"}}{{end}}">
                    </div>

                    <div class="col-3 input-group mb-2 w-50">
                        <div class="input-group-prepend">
                            <div class="input-group-text">To</div>
                        </div>
                        <input type="date" name="service_delivered_date_to" class="form-control"
                               id="ServiceDeliveredDateTo"
                               value="{{if .Options.ServiceDeliveredDateTo}}{{.Options.ServiceDeliveredDateTo.Format "2006-01-02"}}{{end}}">
                    </div>
                </div>
            </div>
            <!-- End Service Delivered Date  -->
        </div>
    </form>

    <div class="card-footer sticky-bottom w-100 bg-white p-5">
        <a href="/countries/{{.RequestContext.SelectedCountry.ID}}/participants"
           class="btn btn-outline-primary">Clear</a>
        <button class="btn btn-primary" type="submit" form="searchForm">Search</button>
    </div>
{{end}}