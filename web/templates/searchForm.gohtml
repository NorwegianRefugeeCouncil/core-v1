{{define "searchForm"}}
    <form method="get" action="/countries/{{.RequestContext.SelectedCountry.ID}}/participants" id="searchForm" class="m-5">
        <input type="hidden" name="take" value="">
        <input type="hidden" name="skip" value="">

        <h6 class="mt-2">Personal Info</h6>
        <div class="row">
            <!-- Full Name -->
            <div class="form-group mb-3 col-3">
                <label class="form-label" for="Name">Name</label>
                <input id="Name"
                       name="full_name"
                       type="text"
                       placeholder="Search by name"
                       class="form-control"
                       value="{{.Options.FullName}}">
            </div>
            <!-- End Full Name -->

            <!-- Sex -->
            <div class="form-group col-3">
                <label class="form-text">Sex</label>
                <select id="Sexes" name="sex" class="form-control">
                    <option value="" {{if .Options.Sexes.IsEmpty }}selected{{end}}>
                    </option>
                    <option value="male"
                            {{if .Options.Sexes.Contains "male" }}selected{{end}}>
                        Male
                    </option>
                    <option value="female"
                            {{if .Options.Sexes.Contains "female" }}selected{{end}}>
                        Female
                    </option>
                    <option value="other"
                            {{if .Options.Sexes.Contains "other" }}selected{{end}}>
                        Other
                    </option>
                    <option value="prefers_not_to_say"
                            {{if .Options.Sexes.Contains "prefers_not_to_say" }}selected{{end}}>
                        Prefers not to say
                    </option>
                </select>
            </div>
            <!-- End Sex -->
        </div>
        <div class="row">

            <!-- Is Minor -->
            <div class="form-group col-3">
                <label class="form-text">Is Minor</label>
                <div>
                    <div class="form-check form-check-inline">
                        <input class="form-check-input" type="radio" value="" id="IsMinorAll"
                               name="is_minor"
                               {{if eq .Options.IsMinor nil }}checked="checked"{{end}}>
                        <label class="form-check-label" for="IsMinorAll">
                            All
                        </label>
                    </div>
                    <div class="form-check form-check-inline">
                        <input class="form-check-input" type="radio" value="true" id="IsMinorYes"
                               name="is_minor"
                               {{if .Options.IsMinorSelected }}checked="checked"{{end}}>
                        <label class="form-check-label" for="IsMinorYes">
                            Yes
                        </label>
                    </div>
                    <div class="form-check form-check-inline mb-3">
                        <input class="form-check-input" type="radio" value="false" id="IsMinorNo"
                               name="is_minor"
                               {{if .Options.IsNotMinorSelected }}checked="checked"{{end}}>
                        <label class="form-check-label" for="IsMinorNo">
                            No
                        </label>
                    </div>
                </div>
            </div>
            <!-- End Is Minor -->

            <!-- Is Minor Headed Household -->
            <div class="form-group col-3">
                <label class="form-text">Is Minor Headed Household</label>
                <div>
                    <div class="form-check form-check-inline">
                        <input class="form-check-input" type="radio" value="" id="IsMinorHoHAll"
                               name="is_minor_headed_household"
                               {{if eq .Options.IsMinorHeadedHousehold nil }}checked="checked"{{end}}>
                        <label class="form-check-label" for="IsMinorHoHAll">
                            All
                        </label>
                    </div>
                    <div class="form-check form-check-inline">
                        <input class="form-check-input" type="radio" value="true" id="IsMinorHoHYes"
                               name="is_minor_headed_household"
                               {{if .Options.IsMinorHeadedHouseholdSelected }}checked="checked"{{end}}>
                        <label class="form-check-label" for="IsMinorHoHYes">
                            Yes
                        </label>
                    </div>
                    <div class="form-check form-check-inline mb-3">
                        <input class="form-check-input" type="radio" value="false" id="IsMinorHoHNo"
                               name="is_minor_headed_household"
                               {{if .Options.IsNotMinorHeadedHouseholdSelected }}checked="checked"{{end}}>
                        <label class="form-check-label" for="IsMinorHoHNo">
                            No
                        </label>
                    </div>
                </div>
            </div>
            <!-- End Is Minor Headed Household -->

            <!-- Is Female Headed Household -->
            <div class="form-group col-3">
                <label class="form-text">Is Female Headed Household</label>
                <div>
                    <div class="form-check form-check-inline">
                        <input class="form-check-input" type="radio" value="" id="IsFemaleHoHAll"
                               name="is_female_headed_household"
                               {{if eq .Options.IsFemaleHeadedHousehold nil }}checked="checked"{{end}}>
                        <label class="form-check-label" for="IsFemaleHoHAll">
                            All
                        </label>
                    </div>
                    <div class="form-check form-check-inline">
                        <input class="form-check-input" type="radio" value="true" id="IsFemaleHoHYes"
                               name="is_female_headed_household"
                               {{if .Options.IsFemaleHeadedHouseholdSelected }}checked="checked"{{end}}>
                        <label class="form-check-label" for="IsFemaleHoHYes">
                            Yes
                        </label>
                    </div>
                    <div class="form-check form-check-inline mb-3">
                        <input class="form-check-input" type="radio" value="false" id="IsFemaleHoHNo"
                               name="is_female_headed_household"
                               {{if .Options.IsNotFemaleHeadedHouseholdSelected }}checked="checked"{{end}}>
                        <label class="form-check-label" for="IsFemaleHoHNo">
                            No
                        </label>
                    </div>
                </div>
            </div>
            <!-- End Is Female Headed Household -->

            <!-- Inactive -->
            <div class="form-group col-3">
                <label class="form-text">Show Inactive</label>
                <div>
                    <div class="form-check form-check-inline">
                        <input class="form-check-input" type="checkbox" value="true" id="Inactive"
                               name="inactive"
                               {{if .Options.IsInactiveSelected }}checked="checked"{{end}}>
                        <label class="form-check-label" for="inactive">
                            Yes
                        </label>
                    </div>
                </div>
            </div>
            <!-- End Inactive -->
        </div>

        <div class="row">
            <!-- ID -->
            <div class="form-group mb-3 col-3">
                <label class="form-label" for="ID">IDs</label>
                <input id="ID"
                       name="id"
                       type="text"
                       placeholder="Search by ID"
                       class="form-control"
                       value="{{if not .Options.IDs.IsEmpty }}{{joinStrings .Options.IDs.Items ","}}{{end}}">
            </div>
            <!-- End ID -->

            <!-- IdentificationNumber -->
            <div class="form-group mb-3 col-3">
                <label class="form-label" for="IdentificationNumber">IdentificationNumber</label>
                <input id="IdentificationNumber"
                       name="identification_number"
                       type="text"
                       placeholder="Search by Identification Number"
                       class="form-control"
                       value="{{.Options.IdentificationNumber}}">
            </div>
            <!-- End IdentificationNumber -->

            <!-- CommunityID -->
            <div class="form-group mb-3 col-3">
                <label class="form-label" for="CommunityID">CommunityID</label>
                <input id="CommunityID"
                       name="community_id"
                       type="text"
                       placeholder="Search by Community ID"
                       class="form-control"
                       value="{{.Options.CommunityID}}">
            </div>
            <!-- End CommunityID -->

            <!-- HouseholdID -->
            <div class="form-group mb-3 col-3">
                <label class="form-label" for="HouseholdID">Household ID</label>
                <input id="HouseholdID"
                       name="household_id"
                       type="text"
                       placeholder="Search by Household ID"
                       class="form-control"
                       value="{{.Options.HouseholdID}}">
            </div>
            <!-- End HouseholdID -->
        </div>

        <div class="row mb-3">
            <!-- Age -->
            <div class="col-6">
                <label class="form-label">Age</label>
                <div class="row">
                    <div class="col-3 input-group mb-2 w-50">
                        <div class="input-group-prepend">
                            <div class="input-group-text">From</div>
                        </div>
                        <input type="number" name="age_from" class="form-control" id="AgeFrom"
                               value="{{if .Options.AgeFrom}}{{.Options.AgeFrom}}{{end}}">
                    </div>

                    <div class="col-3 input-group mb-2 w-50">
                        <div class="input-group-prepend">
                            <div class="input-group-text">To</div>
                        </div>
                        <input type="number" name="age_to" class="form-control" id="AgeTo"
                               value="{{if .Options.AgeTo}}{{.Options.AgeTo}}{{end}}">
                    </div>
                </div>
            </div>
            <!-- End Age -->

            <!-- Birth Date -->
            <div class="col-6">
                <label class="form-label">Birth Date</label>
                <div class="row">
                    <div class="col-3 input-group mb-2 w-50">
                        <div class="input-group-prepend">
                            <div class="input-group-text">From</div>
                        </div>
                        <input type="date" name="birth_date_from" class="form-control"
                               id="BirthDateFrom"
                               value="{{if .Options.BirthDateFrom}}{{.Options.BirthDateFrom.Format "2006-01-02"}}{{end}}">
                    </div>

                    <div class="col-3 input-group mb-2 w-50">
                        <div class="input-group-prepend">
                            <div class="input-group-text">To</div>
                        </div>
                        <input type="date" name="birth_date_to" class="form-control"
                               id="BirthDateTo"
                               value="{{if .Options.BirthDateTo}}{{.Options.BirthDateTo}}{{end}}">
                    </div>
                </div>
            </div>
            <!-- End Birth Date -->
        </div>

        <h6 class="mt-2">Disabilities</h6>
