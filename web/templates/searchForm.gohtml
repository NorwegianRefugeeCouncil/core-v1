            <!-- HasSelfCareDisability -->
            <div class="form-group col-3">
                <label class="form-text">Has Self Care Disability</label>
                <div>
                    <div class="form-check form-check-inline">
                        <input class="form-check-input" type="radio" value=""
                               id="HasSelfCareDisabilityAll"
                               name="has_self_care_disability"
                               {{if eq .Options.HasSelfCareDisability nil }}checked="checked"{{end}}>
                        <label class="form-check-label" for="HasSelfCareDisabilityAll">
