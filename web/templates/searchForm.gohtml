                               name="has_communication_disability"
                               {{if eq .Options.HasCommunicationDisability nil }}checked="checked"{{end}}>
                        <label class="form-check-label" for="HasCommunicationDisabilityAll">
                            All
                        </label>
                    </div>
                    <div class="form-check form-check-inline">
                        <input class="form-check-input" type="radio" value="true"
                               id="HasCommunicationDisabilityYes"
                               name="has_communication_disability"
                               {{if .Options.IsHasCommunicationDisabilitySelected }}checked="checked"{{end}}>
                        <label class="form-check-label" for="HasCommunicationDisabilityYes">
                            Yes
                        </label>
                    </div>
                    <div class="form-check form-check-inline mb-3">
                        <input class="form-check-input" type="radio" value="false"
                               id="HasCommunicationDisabilityNo"
                               name="has_communication_disability"
                               {{if .Options.IsNotHasCommunicationDisabilitySelected }}checked="checked"{{end}}>
                        <label class="form-check-label" for="HasCommunicationDisabilityNo">
                            No
                        </label>
                    </div>
                </div>
            </div>
            <!-- End HasCommunicationDisability -->

            <!-- HasHearingDisability -->
            <div class="form-group col-3">
                <label class="form-text">Has Hearing Disability</label>
                <div>
                    <div class="form-check form-check-inline">
                        <input class="form-check-input" type="radio" value=""
                               id="HasHearingDisabilityAll"
                               name="has_hearing_disability"
                               {{if eq .Options.HasHearingDisability nil }}checked="checked"{{end}}>
                        <label class="form-check-label" for="HasHearingDisabilityAll">
                            All
                        </label>
                    </div>
                    <div class="form-check form-check-inline">
                        <input class="form-check-input" type="radio" value="true"
                               id="HasHearingDisabilityYes"
                               name="has_hearing_disability"
                               {{if .Options.IsHasHearingDisabilitySelected }}checked="checked"{{end}}>
                        <label class="form-check-label" for="HasHearingDisabilityYes">
                            Yes
                        </label>
                    </div>
                    <div class="form-check form-check-inline mb-3">
                        <input class="form-check-input" type="radio" value="false"
                               id="HasHearingDisabilityNo"
                               name="has_hearing_disability"
                               {{if .Options.IsNotHasHearingDisabilitySelected }}checked="checked"{{end}}>
                        <label class="form-check-label" for="HasHearingDisabilityNo">
                            No
                        </label>
                    </div>
                </div>
            </div>
            <!-- End HasHearingDisability -->

            <!-- HasMobilityDisability -->
            <div class="form-group col-3">
                <label class="form-text">Has Mobility Disability</label>
                <div>
                    <div class="form-check form-check-inline">
                        <input class="form-check-input" type="radio" value=""
                               id="HasMobilityDisabilityAll"
                               name="has_mobility_disability"
                               {{if eq .Options.HasMobilityDisability nil }}checked="checked"{{end}}>
                        <label class="form-check-label" for="HasMobilityDisabilityAll">
                            All
                        </label>
                    </div>
                    <div class="form-check form-check-inline">
                        <input class="form-check-input" type="radio" value="true"
                               id="HasMobilityDisabilityYes"
                               name="has_mobility_disability"
                               {{if .Options.IsHasMobilityDisabilitySelected }}checked="checked"{{end}}>
                        <label class="form-check-label" for="HasMobilityDisabilityYes">
                            Yes
                        </label>
                    </div>
                    <div class="form-check form-check-inline mb-3">
                        <input class="form-check-input" type="radio" value="false"
                               id="HasMobilityDisabilityNo"
                               name="has_mobility_disability"
