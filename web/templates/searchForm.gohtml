{{define "searchForm"}}
    <form method="get" action="/countries/{{.RequestContext.SelectedCountry.ID}}/participants" id="searchForm" class="m-5">
        <input type="hidden" name="take" value="">
        <input type="hidden" name="skip" value="">

        <h6 class="mt-2">
            {{.RequestContext.Locales.Translate "personal_info"}}
        </h6>
        <div class="row">
            <!-- Full Name -->
            <div class="form-group mb-3 col-3">
                <label class="form-label" for="Name">
                    {{.RequestContext.Locales.Translate "name"}}
                </label>
                <input id="Name"
                       name="full_name"
                       type="text"
                       placeholder="{{.RequestContext.Locales.Translate "search_placeholder" (.RequestContext.Locales.Translate "name")}}"
                       class="form-control"
                       value="{{.Options.FullName}}">
            </div>
            <!-- End Full Name -->

            <!-- Mothers Name -->
            <div class="form-group mb-3 col-3">
                <label class="form-label" for="MothersName">
                    {{.RequestContext.Locales.Translate "mother_name"}}
                </label>
                <input id="MothersName"
                       name="mothers_name"
                       type="text"
                       placeholder="{{.RequestContext.Locales.Translate "search_placeholder" (.RequestContext.Locales.Translate "mother_name")}}"
                       class="form-control"
                       value="{{.Options.MothersName}}">
            </div>
            <!-- End Mothers Name -->

            <!-- Sex -->
            <div class="form-group col-3">
                <label class="form-text">
                    {{.RequestContext.Locales.Translate "sex"}}
                </label>
                <select id="Sexes" name="sex" class="form-control">
                    <option value="" {{if .Options.Sexes.IsEmpty }}selected{{end}}>
                        {{.RequestContext.Locales.Translate "all"}}
                    </option>
                    <option value="male"
                            {{if .Options.Sexes.Contains "male" }}selected{{end}}>
                        {{.RequestContext.Locales.Translate "sex_male"}}
                    </option>
                    <option value="female"
                            {{if .Options.Sexes.Contains "female" }}selected{{end}}>
                        {{.RequestContext.Locales.Translate "sex_female"}}
                    </option>
                    <option value="other"
                            {{if .Options.Sexes.Contains "other" }}selected{{end}}>
                        {{.RequestContext.Locales.Translate "other_option"}}
                    </option>
                    <option value="prefers_not_to_say"
                            {{if .Options.Sexes.Contains "prefers_not_to_say" }}selected{{end}}>
                        {{.RequestContext.Locales.Translate "prefers_not_to_say"}}
                    </option>
                </select>
            </div>
            <!-- End Sex -->
        </div>
        <div class="row">

            <!-- Is Minor -->
            <div class="form-group col-3">
                <label class="form-text">
                    {{.RequestContext.Locales.Translate "is_minor"}}
                </label>
                <div>
                    <div class="form-check form-check-inline">
                        <input class="form-check-input" type="radio" value="" id="IsMinorAll"
                               name="is_minor"
                               {{if eq .Options.IsMinor nil }}checked="checked"{{end}}>
                        <label class="form-check-label" for="IsMinorAll">
                            {{.RequestContext.Locales.Translate "all"}}
                        </label>
                    </div>
                    <div class="form-check form-check-inline">
                        <input class="form-check-input" type="radio" value="true" id="IsMinorYes"
                               name="is_minor"
                               {{if .Options.IsMinorSelected }}checked="checked"{{end}}>
                        <label class="form-check-label" for="IsMinorYes">
                            {{.RequestContext.Locales.Translate "yes"}}
                        </label>
                    </div>
                    <div class="form-check form-check-inline mb-3">
                        <input class="form-check-input" type="radio" value="false" id="IsMinorNo"
                               name="is_minor"
                               {{if .Options.IsNotMinorSelected }}checked="checked"{{end}}>
                        <label class="form-check-label" for="IsMinorNo">
                            {{.RequestContext.Locales.Translate "no"}}
                        </label>
                    </div>
                </div>
            </div>
            <!-- End Is Minor -->

            <!-- Is Minor Headed Household -->
