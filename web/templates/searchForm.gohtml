                    <div class="form-check form-check-inline mb-3">
                        <input class="form-check-input" type="radio" value="false"
                               id="HasVisionDisabilityNo"
                               name="has_vision_disability"
                               {{if .Options.IsNotHasVisionDisabilitySelected }}checked="checked"{{end}}>
                        <label class="form-check-label" for="HasVisionDisabilityNo">
