                        <label class="form-check-label" for="IsInactive">
                            {{.RequestContext.Locales.Translate "yes"}}
                        </label>
                    </div>
                    <div class="form-check form-check-inline mb-3">
                        <input class="form-check-input" type="radio" value="false" id="IsNotInactive"
                               name="inactive"
                               {{if .Options.IsNotInactiveSelected }}checked="checked"{{end}}>
                        <label class="form-check-label" for="IsNotInactive">
                            {{.RequestContext.Locales.Translate "no"}}
                        </label>
